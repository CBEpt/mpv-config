[
    {
      "label": "打开文件浏览器",
      "command": ["script_message", "browse-files"],
      "keep_open": false
    },
    {
      "label": "退出",
      "command": ["quit"]
    },
    {
      "label": "3840x2160 60Hz",
      "command": ["script-message","change-refresh","3840","2160","60"]
    },
    {
      "label": "切换Tscale-box配置",
      "command": [["apply-profile", "Tscale-box"], ["show-text", "Tscale-box"]],
    }, 
    {
      "label": "切换DeBand-high配置",
      "command": [["apply-profile", "DeBand-high"], ["show-text", "DeBand-high"]],
    },
    {
      "label": "切换Anime-ravu-3x配置",
      "command": [["apply-profile", "Anime-ravu-3x"], ["show-text", "Anime-ravu-3x"]],
    },
    {
      "label": "重置所有更改设置",
      "command": [["cycle_values", "reset-on-next-file", "all"], ["show-text", "重置所有更改设置"]],
      "keep_open": false
    },
    {
      "label": "更新脚本着色器",
      "command": [["script-binding", "manager-update-all"], ["show-text", "更新脚本着色器"]]
    }
  ]